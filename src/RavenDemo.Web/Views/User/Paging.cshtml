@model PagedList<User>
@{
    ViewBag.Title = "Users";
}

<h1>Users</h1>
<table class="table table-hover">
    <thead>
        <tr>
            <th>Username</th>
            <th>Email</th>
            <th>Name</th>
        </tr>
    </thead>
    <tbody>
        @foreach (User user in Model.List)
        {
            <tr>
                <td>@user.Username</td>
                <td>@user.Email</td>
                <td>@user.Name</td>
            </tr>
        }
    </tbody>
</table>
@Html.Pagination(Model)